#![GameMap.biggestOf(Queue<Village> villages)]
tile:Tile
map:GameMap
village:Village

tile:currentVillage=map.biggestOf(villages)
map:currentVillage = villages\.poll()
[c: loop: for each village in villages]
map:village.isBigger:=compareTo(Village v)
